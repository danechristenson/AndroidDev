<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/view_review_AliasTextView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:text="Alias" />

    <TextView
        android:id="@+id/view_review_CategoryTextView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:text="Category" />

    <TextView
        android:id="@+id/view_review_DescriptionTextView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:text="Description" />

    <TextView
        android:id="@+id/view_review_AddInfoTextView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:text="Additional Information" />

    <TextView
        android:id="@+id/view_review_ReviewTextView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:text="Review" />
</LinearLayout>